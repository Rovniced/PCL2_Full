<Border
	x:Class="PCL.MyRadioButton" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	Name="PanBack"
	HorizontalAlignment="Center"
	VerticalAlignment="Center"
	Background="{StaticResource ColorBrushSemiTransparent}"
	CornerRadius="13.5"
	MinHeight="27"
	MaxHeight="27" xmlns:pcl="clr-namespace:PCL">
	<StackPanel
		Orientation="Horizontal">
		<!--未知连接 ID：2-->
		<Path
			Name="ShapeLogo"
			HorizontalAlignment="Left"
			RenderTransformOrigin="0.5,0.5"
			Margin="12,0,0,0"
			Stretch="Uniform"
			Fill="#FFFFFFFF"
			MaxHeight="16"
			MaxWidth="16"
			VerticalAlignment="Center" />
		<!--未知连接 ID：3-->
		<TextBlock
			Name="LabText"
			VerticalAlignment="Center"
			Grid.Column="1"
			Foreground="#FFFFFFFF"
			Margin="8,0,12,0"
			Grid.ColumnSpan="2"
			SnapsToDevicePixels="False"
			UseLayoutRounding="False" />
	</StackPanel>
</Border>
