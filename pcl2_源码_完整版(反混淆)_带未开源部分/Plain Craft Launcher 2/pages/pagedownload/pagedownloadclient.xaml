<AdornerDecorator
	x:Class="PCL.PageDownloadClient" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:local="clr-namespace:PCL" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006">
	<Grid>
		<!--未知连接 ID：1-->
		<local:MyScrollViewer
			Name="PanBack"
			Opacity="0"
			VerticalScrollBarVisibility="Auto"
			HorizontalScrollBarVisibility="Disabled"
			Visibility="Collapsed">
			<StackPanel
				Orientation="Vertical"
				Margin="5">
				<!--未知连接 ID：2-->
				<StackPanel
					Name="PanMain"
					Margin="20,20,20,5"
					Grid.IsSharedSizeScope="True" />
			</StackPanel>
		</local:MyScrollViewer>
		<!--未知连接 ID：3-->
		<local:MyCard
			Name="PanLoad"
			HorizontalAlignment="Center"
			VerticalAlignment="Center"
			Margin="40,0"
			LoadAnimation="False"
			SnapsToDevicePixels="True"
			UseAnimation="False"
			Opacity="0"
			Visibility="Collapsed">
			<!--未知连接 ID：4-->
			<local:MyLoading
				Name="Load"
				Text="正在获取版本列表"
				Margin="20,20,20,17"
				HorizontalAlignment="Center"
				VerticalAlignment="Center" />
		</local:MyCard>
	</Grid>
</AdornerDecorator>
